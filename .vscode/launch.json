{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch template",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "target": "${workspaceFolder}",
            "terminal": "integrated",
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}",
            },
            "args": [
                "--nostatusrc",
                "--outputdir", "output", 
                "--variable", "BROWSER:chrome",
                "--include", "tnr",
            ],
        }
    ]
}